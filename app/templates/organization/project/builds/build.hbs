{{title model.buildTitle}}
{{fixed-top-build-header project=model.project organization=model.project.organization}}
{{build-container
  build=model
  activeSnapshotId=activeSnapshotId
  updateActiveSnapshotId=(route-action "updateActiveSnapshotId")
  showSupport="showSupport"
  openSnapshotFullModal='openSnapshotFullModal'
  isShowingModal=model.isShowingModal
}}
{{outlet}}
